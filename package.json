{
	"name": "aicowork",
	"private": true,
	"version": "0.1.0",
	"description": "AI Cowork - 智能协作助手，支持多厂商 AI API、记忆系统和斜杠命令",
	"author": "Alan <none>",
	"type": "module",
	"main": "dist-electron/electron/main.js",
	"scripts": {
		"postinstall": "electron-builder install-app-deps",
		"rebuild": "npx electron-rebuild -f -w better-sqlite3",
		"dev": "run-p dev:vite dev:electron",
		"dev:vite": "vite",
		"dev:electron": "run-s transpile:electron electron:start",
		"electron:start": "cross-env NODE_ENV=development electron .",
		"dev:react": "vite",
		"build": "run-s transpile:electron vite:build",
		"vite:build": "tsc -b && vite build",
		"lint": "eslint .",
		"preview": "vite preview",
		"transpile:electron": "tsc --project src/electron/tsconfig.json",
		"dist": "run-s build electron-builder",
		"electron-builder": "electron-builder --publish never",
		"dist:mac-arm64": "run-s build && electron-builder --mac --arm64",
		"dist:mac-x64": "run-s build && electron-builder --mac --x64",
		"dist:win": "run-s build && electron-builder --win --x64",
		"dist:linux": "run-s build && electron-builder --linux --x64",
		"test": "vitest run",
		"test:watch": "vitest",
		"test:coverage": "vitest run --coverage",
		"test:components": "vitest run --config vitest.config.components.ts"
	},
	"dependencies": {
		"@anthropic-ai/tokenizer": "^0.0.4",
		"@modelcontextprotocol/sdk": "^1.25.3",
		"@qwen-code/sdk": "0.1.5-preview.1",
		"@radix-ui/react-dialog": "^1.1.15",
		"@radix-ui/react-dropdown-menu": "^2.1.16",
		"@radix-ui/react-tooltip": "^1.2.8",
		"@tailwindcss/vite": "^4.1.18",
		"adm-zip": "^0.5.16",
		"axios": "^1.13.5",
		"better-sqlite3": "^12.6.2",
		"dingtalk-stream": "^2.1.4",
		"highlight.js": "^11.11.1",
		"i18next": "^25.7.4",
		"lucide-react": "^0.562.0",
		"react": "^19.2.3",
		"react-dom": "^19.2.3",
		"react-i18next": "^16.5.3",
		"react-markdown": "^10.1.0",
		"rehype-highlight": "^7.0.2",
		"rehype-raw": "^7.0.0",
		"remark-gfm": "^4.0.1",
		"tailwindcss": "^4.1.18",
		"vite-tsconfig-paths": "^6.0.4",
		"winston": "^3.19.0",
		"zod": "^4.3.5",
		"zustand": "^5.0.10"
	},
	"devDependencies": {
		"@eslint/js": "^9.39.2",
		"@testing-library/jest-dom": "^6.9.1",
		"@testing-library/react": "^16.3.2",
		"@testing-library/user-event": "^14.6.1",
		"@types/adm-zip": "^0.5.7",
		"@types/better-sqlite3": "^7.6.13",
		"@types/node": "^25.2.2",
		"@types/react": "^19.2.8",
		"@types/react-dom": "^19.2.3",
		"@vitejs/plugin-react": "^5.1.2",
		"@vitest/ui": "^4.0.17",
		"cross-env": "^10.1.0",
		"electron": "^39.2.7",
		"electron-builder": "^26.4.0",
		"eslint": "^9.39.2",
		"eslint-plugin-react-hooks": "^7.0.1",
		"eslint-plugin-react-refresh": "^0.4.26",
		"globals": "^17.0.0",
		"happy-dom": "^20.3.4",
		"npm-run-all": "^4.1.5",
		"typescript": "~5.9.3",
		"typescript-eslint": "^8.53.1",
		"vite": "^7.3.1",
		"vitest": "^4.0.17"
	},
	"pnpm": {
		"peerDependencyRules": {
			"ignoreMissing": [
				"zod"
			]
		}
	}
}
